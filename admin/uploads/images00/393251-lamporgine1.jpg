<script type="text/javascript">
    var userSummary;
    var userSummaryValue = localStorage.getItem("cars.userSummary");
    if (userSummaryValue) {
        try {
            userSummary = JSON.parse(userSummaryValue);
        } catch(e) {}
    }

    var carsUserDataValue, listingId;
    carsUserDataValue = localStorage.getItem("Cars.userData");
    if (carsUserDataValue) {
        try {
            carsUserData = JSON.parse(carsUserDataValue);
            listingId = carsUserData.listingId;
        } catch(e) {}
    }

    if (userSummary && userSummary.email) {
        _etmc.push(["setOrgId", "Carscomconsumer"]);
        _etmc.push(["setUserInfo", {"email": userSummary.email}]);

        let pageViewList = ["trackPageView"];
        if (listingId) {
            pageViewList.push({"item": ""+listingId});
        }
        _etmc.push(pageViewList);
    }
</script>

<script type="text/javascript">
    setAccordion(); // menu expand on/off based on size
    window.addEventListener("resize", setAccordion);

    function setAccordion() { // using footer_accordion_input to differentiate from other accordions
        let accordions = window.document.documentElement.getElementsByClassName("footer_accordion_input");
        let headers = window.document.documentElement.getElementsByClassName("footer_accordion_header");
        let waist = window.innerWidth;
    
        if (accordions && headers) {
            for (let m = 0; m < accordions.length; m++) {
                if (waist < 504) { // mobile first
                    headers[m].style.cursor = "pointer";
                    accordions[m].checked = false;
                    accordions[m].disabled = false;
                } else { // larger screens
                    headers[m].style.cursor = "default";
                    accordions[m].checked = true;
                    accordions[m].disabled = true;
                }
            }
        }
    }
</script>



